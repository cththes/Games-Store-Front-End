{
   "extends":[
      "react-app"
   ],
   "rules":{
      "no-console":"error",
      "prefer-const":"error",
      "quotes":["error", "single"],
      "jsx-quotes":["error", "prefer-double"],
      "intend":["warn", 2],
      "max-len":["error", {"code":120}],
      "comma-dangle":["error", "always-multiline"],
      "semi":["warn", "always"],
      "import/order":["error",{
         "groups":["builtin", "external","internal",
         "parent", "sibing", "index", "object","type"],
         "newlines-between":"always-and-inside-groups"
      }]
   }
}